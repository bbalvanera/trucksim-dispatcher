{
  "name": "trucksim-dispatcher",
  "version": "0.1.0",
  "private": true,
  "main": "dist/main.js",
  "scripts": {
    "start": "concurrently \"cross-env npm run craco-start\" \"wait-on http://localhost:3000 && electron .\"",
    "craco-start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "electron": "electron .",
    "compile": "tsc --project electron/tsconfig.json --outdir dist",
    "pretty": "prettier -c \"src/**/*.{js,jsx,ts,tsx,json,css,scss}\"",
    "pretty:fix": "prettier -w \"src/**/*.{js,jsx,ts,tsx,json,css,scss}\"",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "precommit": "npm run pretty & npm run lint",
    "prepare": "husky"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.9",
    "@mui/material": "^5.15.9",
    "csstype": "^3.1.3",
    "electron": "^28.2.2",
    "i18next": "^23.8.2",
    "inline-style-expand-shorthand": "^1.6.0",
    "lodash.merge": "^4.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.0.5",
    "react-router-dom": "^6.22.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4",
    "winreg": "^1.2.5"
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/lodash.merge": "^4.6.9",
    "@types/node": "^16.18.80",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@types/winreg": "^1.2.36",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.3",
    "electron-is-dev": "^3.0.1",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "wait-on": "^7.2.0"
  }
}
